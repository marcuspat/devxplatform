# Environment
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Database (PostgreSQL with Prisma)
DATABASE_URL="postgresql://username:password@localhost:5432/nextapp?schema=public"

# NextAuth.js
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-key-here

# OAuth Providers (Examples)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# Email (for NextAuth.js email provider)
EMAIL_SERVER_HOST=
EMAIL_SERVER_PORT=587
EMAIL_SERVER_USER=
EMAIL_SERVER_PASSWORD=
EMAIL_FROM=

# Analytics (Examples)
NEXT_PUBLIC_GA_MEASUREMENT_ID=
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=

# Monitoring
SENTRY_DSN=
SENTRY_ORG=
SENTRY_PROJECT=

# Storage (for file uploads)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=
AWS_S3_BUCKET=

# Redis (for caching/sessions)
REDIS_URL=redis://localhost:6379

# Feature Flags
NEXT_PUBLIC_FEATURE_NEW_DASHBOARD=false
NEXT_PUBLIC_FEATURE_BETA_FEATURES=false